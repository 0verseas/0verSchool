	<script>
    function openLoading() {
      $('body').loading();
      $('.loading-overlay').css({'z-index': 11111, top: 0});
    }

    function stopLoading() {
      $('body').loading('stop');
    }

    openLoading();

	if(!Browser.checkBrowserSupport()) {
		alert('請使用 Chrome, Firefox, Safari 或 Edge 版本 > 14 的瀏覽器。');
		window.history.back();
	}

	if (!env.isProduction) {
		document.title='簡章調查系統-測試站';
	}
