<div class="container" id="page-user">
	<div class="row">
		<div class="col-lg-8 col-md-12 offset-lg-2">
			<div class="card" id="userForm">
				<div class="card-header" data-i18n="user.title"><!-- 編輯個人資料 --></div>
				<div class="card-block">
					<div class="form-group row">
						<label class="col-2 col-form-label" data-i18n="user.form.school"><!-- 學校 --></label>
						<div class="col-10">
							<input class="form-control" type="text" disabled />
						</div>
					</div>
					<div class="form-group row">
						<label for="" class="col-2 col-form-label" data-i18n="user.form.cname"><!-- 中文姓名 --></label>
						<div class="col-10">
							<input class="form-control" type="text" />
						</div>
					</div>
					<div class="form-group row">
						<label for="" class="col-2 col-form-label" data-i18n="user.form.ename"><!-- 英文姓名 --></label>
						<div class="col-10">
							<input class="form-control" type="text" />
						</div>
					</div>
					<div class="form-group row">
						<label for="" class="col-2 col-form-label" data-i18n="user.form.email"><!-- 信箱 --></label>
						<div class="col-10">
							<input class="form-control" type="text" />
						</div>
					</div>
					<div class="form-group row">
						<label for="" class="col-2 col-form-label" data-i18n="user.form.pass"><!-- 密碼 --></label>
						<div class="col-10">
							<input class="form-control" type="password" />
						</div>
					</div>
					<div class="form-group row">
						<label for="" class="col-2 col-form-label" data-i18n="user.form.org"><!-- 處室 --></label>
						<div class="col-10">
							<input class="form-control" type="text" />
						</div>
					</div>
					<div class="form-group row">
						<label for="" class="col-2 col-form-label" data-i18n="user.form.tel"><!-- 電話 --></label>
						<div class="col-10">
							<input class="form-control" type="text" />
						</div>
					</div>
					<div class="form-group row">
						<label for="" class="col-2 col-form-label"></label>
						<div class="col-10">
							<button id="btn-updateUser" type="button" class="btn btn-primary" data-i18n="user.form.update"><!-- 更新 --></button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
</div>
